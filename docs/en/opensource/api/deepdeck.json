  "components": {
    "schemas": {
      "LayerDataArray": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "example": [
              {
                "name": "Media",
                "pos": 0,
                "active": true,
                "uuid": "62d8ba"
              },
              {
                "name": "Numpad",
                "pos": 1,
                "active": true,
                "uuid": "8531d4"
              },
              {
                "name": "Chrome",
                "pos": 2,
                "active": false,
                "uuid": "17df94"
              }
            ],
            "items": {
              "$ref": "#/components/schemas/LayerInfo"
            }
          }
        }
      },
      "LayerInfo": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Media"
          },
          "pos": {
            "type": "integer",
            "example": 0
          },
          "active": {
            "type": "boolean",
            "example": true
          },
          "uuid": {
            "type": "string",
            "example": "62d8ba"
          }
        }
      },
      "Layer": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Media"
          },
          "active": {
            "type": "boolean"
          },
          "row0": {
            "type": "array",
            "example": [
              {
                "name": "Play",
                "key_code": 171
              },
              {
                "name": "next",
                "key_code": 168
              },
              {
                "name": "mute",
                "key_code": 172
              },
              {
                "name": "layer",
                "key_code": 402
              }
            ],
            "items": {
              "$ref": "#/components/schemas/Layer_row0"
            }
          },
          "row1": {
            "type": "array",
            "example": [
              {
                "name": "nWind",
                "key_code": 502
              },
              {
                "name": "PWind",
                "key_code": 503
              },
              {
                "name": "F11",
                "key_code": 68
              },
              {
                "name": "PrtSC",
                "key_code": 70
              }
            ],
            "items": {
              "$ref": "#/components/schemas/Layer_row1"
            }
          },
          "row2": {
            "type": "array",
            "example": [
              {
                "name": "Copy",
                "key_code": 500
              },
              {
                "name": "Paste",
                "key_code": 501
              },
              {
                "name": "up",
                "key_code": 82
              },
              {
                "name": "Enter",
                "key_code": 40
              }
            ],
            "items": {
              "$ref": "#/components/schemas/Layer_row2"
            }
          },
          "row3": {
            "type": "array",
            "example": [
              {
                "name": "window",
                "key_code": 227
              },
              {
                "name": "left",
                "key_code": 80
              },
              {
                "name": "down",
                "key_code": 81
              },
              {
                "name": "right",
                "key_code": 79
              }
            ],
            "items": {
              "$ref": "#/components/schemas/Layer_row3"
            }
          },
          "left_encoder_map": {
            "$ref": "#/components/schemas/Layer_left_encoder_map"
          },
          "right_encoder_map": {
            "$ref": "#/components/schemas/Layer_right_encoder_map"
          },
          "gesture_map": {
            "$ref": "#/components/schemas/Layer_gesture_map"
          }
        }
      },
      "LayerWithID": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Media"
          },
          "active": {
            "type": "boolean"
          },
          "uuid": {
            "type": "string",
            "example": "4e7a48abf8"
          },
          "row0": {
            "type": "array",
            "example": [
              {
                "name": "Play",
                "key_code": 171
              },
              {
                "name": "next",
                "key_code": 168
              },
              {
                "name": "mute",
                "key_code": 172
              },
              {
                "name": "layer",
                "key_code": 402
              }
            ],
            "items": {
              "$ref": "#/components/schemas/Layer_row0"
            }
          },
          "row1": {
            "type": "array",
            "example": [
              {
                "name": "nWind",
                "key_code": 502
              },
              {
                "name": "PWind",
                "key_code": 503
              },
              {
                "name": "F11",
                "key_code": 68
              },
              {
                "name": "PrtSC",
                "key_code": 70
              }
            ],
            "items": {
              "$ref": "#/components/schemas/Layer_row1"
            }
          },
          "row2": {
            "type": "array",
            "example": [
              {
                "name": "Copy",
                "key_code": 500
              },
              {
                "name": "Paste",
                "key_code": 501
              },
              {
                "name": "up",
                "key_code": 82
              },
              {
                "name": "Enter",
                "key_code": 40
              }
            ],
            "items": {
              "$ref": "#/components/schemas/Layer_row2"
            }
          },
          "row3": {
            "type": "array",
            "example": [
              {
                "name": "window",
                "key_code": 227
              },
              {
                "name": "left",
                "key_code": 80
              },
              {
                "name": "down",
                "key_code": 81
              },
              {
                "name": "right",
                "key_code": 79
              }
            ],
            "items": {
              "$ref": "#/components/schemas/Layer_row3"
            }
          },
          "left_encoder_map": {
            "$ref": "#/components/schemas/Layer_left_encoder_map"
          },
          "right_encoder_map": {
            "$ref": "#/components/schemas/Layer_right_encoder_map"
          },
          "gesture_map": {
            "$ref": "#/components/schemas/Layer_gesture_map"
          }
        }
      },
      "Macros": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "keycode": {
            "type": "integer",
            "format": "int16"
          },
          "key": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          }
        },
        "example": {
          "name": "Copy",
          "keycode": 500,
          "key": [
            224,
            6,
            0,
            0,
            0
          ]
        }
      },
      "MacrosList": {
        "type": "object",
        "properties": {
          "macros": {
            "type": "array",
            "example": [
              {
                "name": "Copy",
                "keycode": 500,
                "key": [
                  224,
                  6,
                  0,
                  0,
                  0
                ]
              },
              {
                "name": "Paste",
                "keycode": 501,
                "key": [
                  224,
                  25,
                  0,
                  0,
                  0
                ]
              },
              {
                "name": "AltTab",
                "keycode": 502,
                "key": [
                  226,
                  43,
                  0,
                  0,
                  0
                ]
              }
            ],
            "items": {
              "$ref": "#/components/schemas/Macros"
            }
          }
        }
      },
      "Tapdance": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "tap_list": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          },
          "keycode_list": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          },
          "keycode": {
            "type": "integer"
          }
        },
        "example": {
          "name": "Media",
          "tap_list": [
            1,
            2,
            3,
            4
          ],
          "keycode_list": [
            171,
            168,
            169,
            172
          ],
          "keycode": 601
        }
      },
      "TapdanceList": {
        "type": "object",
        "properties": {
          "tapdance": {
            "type": "array",
