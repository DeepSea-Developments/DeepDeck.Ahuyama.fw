  "paths": {
    "/api/layers/layer_names": {
      "get": {
        "tags": [
          "layers"
        ],
        "summary": "Return main information of existing layers",
        "description": "Return an array with the name, position, id, and active flag of the existing layers\n",
        "operationId": "getLAyerNames",
        "responses": {
          "200": {
            "description": "List of layer returned.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LayerDataArray"
                }
              }
            }
          }
        }
      }
    },
    "/api/layers": {
      "get": {
        "tags": [
          "layers"
        ],
        "summary": "Return information of the selected layer",
        "description": "Return the content of the layer\n",
        "operationId": "getLayer",
        "parameters": [
          {
            "name": "uuid",
            "in": "query",
            "description": "The layer ID",
            "required": true,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "62d8ba"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Layer returned",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Layer"
                }
              }
            }
          },
          "400": {
            "description": "Layer not found"
          }
        }
      },
      "put": {
        "tags": [
          "layers"
        ],
        "summary": "Update layer",
        "description": "Update layer\n",
        "operationId": "updateLayer",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LayerWithID"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Layer returned",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Layer"
                }
              }
            }
          },
          "400": {
            "description": "Json not found or not valid"
          }
        }
      },
      "post": {
        "tags": [
          "layers"
        ],
        "summary": "Create new layer",
        "description": "Create a new layer\n",
        "operationId": "createLayer",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Layer"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Layer returned",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Layer"
                }
              }
            }
          },
          "400": {
            "description": "Json not found or not valid"
          }
        }
      },
      "delete": {
        "tags": [
          "layers"
        ],
        "summary": "Delete the selected layer",
        "description": "Delete the selected layer\n",
        "operationId": "deleteLayer",
        "parameters": [
          {
            "name": "uuid",
            "in": "query",
            "description": "The layer ID",
            "required": true,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "62d8ba"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Layer deleted succesfully"
          },
          "400": {
            "description": "Layer id not found"
          }
        }
      }
    },
