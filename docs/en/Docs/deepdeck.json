{
  "openapi": "3.0.0",
  "info": {
    "title": "DeepDeck Ahuyama API",
    "description": "DeepDeck configuration API",
    "contact": {
      "email": "deepdeck@dsd.dev"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://virtserver.swaggerhub.com/NICOLASVELASQUEZ531/DeepDeck/1.0.0",
      "description": "SwaggerHub API Auto Mocking"
    },
    {
      "url": "http://192.168.4.1",
      "description": "Local DeepDeck connected"
    },
    {
      "url": "http://{DeepDeckIP}",
      "description": "DeepDeck",
      "variables": {
        "DeepDeckIP": {
          "description": "IP of DeepDeck",
          "default": "192.168.4.1"
        }
      }
    }
  ],
  "tags": [
    {
      "name": "layers",
      "description": "Layer-related calls"
    },
    {
      "name": "developers",
      "description": "Operations available to regular developers"
    }
  ],
  "paths": {
    "/api/layers/layer_names": {
      "get": {
        "tags": [
          "layers"
        ],
        "summary": "Return main information of existing layers",
        "description": "Return an array with the name, position, id, and active flag of the existing layers\n",
        "operationId": "getLAyerNames",
        "responses": {
          "200": {
            "description": "search results matching criteria"
          },
          "400": {
            "description": "bad input parameter"
          }
        }
      },
      "post": {
        "tags": [
          "admins"
        ],
        "summary": "adds an inventory item",
        "description": "Adds an item to the system",
        "operationId": "addInventory",
        "requestBody": {
          "description": "Inventory item to add",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/InventoryItem"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "item created"
          },
          "400": {
            "description": "invalid input, object invalid"
          },
          "409": {
            "description": "an existing item already exists"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "InventoryItem": {
        "required": [
          "id",
          "manufacturer",
          "name",
          "releaseDate"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid",
            "example": "d290f1ee-6c54-4b01-90e6-d701748f0851"
          },
          "name": {
            "type": "string",
            "example": "Widget Adapter"
          },
          "releaseDate": {
            "type": "string",
            "format": "date-time",
            "example": "2016-08-29T09:12:33.001Z"
          },
          "manufacturer": {
            "$ref": "#/components/schemas/Manufacturer"
          }
        }
      },
      "Manufacturer": {
        "required": [
          "name"
        ],
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "ACME Corporation"
          },
          "homePage": {
            "type": "string",
            "format": "url",
            "example": "https://www.acme-corp.com"
          },
          "phone": {
            "type": "string",
            "example": "408-867-5309"
          }
        }
      }
    }
  }
}